
<div class="container-dog-index">
<div class= "row no-gutter-dog-index">
  <% @dogs.each do |dog| %>
    <% if dog.photo? %>
  <div class="col-lg-3">
      <%= link_to dog_path(dog) do %>
       <%= cl_image_tag dog.photo.path, width: 200, height: 200, crop: :fill  %>
      <% end %>
    <% end %>
  </div>
  <% end %>
</div>
</div>
